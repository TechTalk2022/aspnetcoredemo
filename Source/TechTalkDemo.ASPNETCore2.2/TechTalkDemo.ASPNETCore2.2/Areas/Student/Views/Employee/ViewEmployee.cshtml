@model TechTalkDemo.ASPNETCore2._2.Areas.Student.Models.EmployeeModel;
<h1>View Employee Details</h1>

<table class="table">
    <thead>
    <th>ID</th>
    <th>First Name</th>
    <th>Last Name </th>
    </thead>
    <tbody>
        @foreach (var item in Model.EmployeeLst)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
            </tr>
        }
    </tbody>

</table>